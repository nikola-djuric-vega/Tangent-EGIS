{
  "name": "egis",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run i18n && next build",
    "start": "NODE_ENV=production node server.js",
    "lint": "next lint",
    "storybook": "start-storybook -s ./public,./.storybook -p 6006",
    "build-storybook": "build-storybook",
    "extract:i18n": "formatjs extract '{pages,containers,components}/**/*.{js,ts,tsx}' --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file translations/extracted-locales/en.json",
    "compile:i18n": "formatjs compile-folder --ast translations/extracted-locales translations/compiled-locales",
    "i18n": "npm run extract:i18n && npm run compile:i18n",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "{components,containers,pages}/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "__tests__/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.4.5",
    "@azure/identity": "^2.0.2",
    "@azure/logger": "^1.0.3",
    "@azure/ms-rest-nodeauth": "^3.1.1",
    "@azure/search-documents": "^11.2.1",
    "@azure/service-bus": "^7.4.0",
    "@formatjs/cli": "^4.2.27",
    "@headlessui/react": "^1.4.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/lodash": "^4.14.172",
    "@types/node": "^17.0.16",
    "@types/react": "17.0.11",
    "axios": "^0.23.0",
    "babel-plugin-formatjs": "^10.3.2",
    "bingmaps-react": "^1.1.5",
    "clsx": "^1.1.1",
    "formik": "^2.2.9",
    "framer-motion": "^4.1.17",
    "google-map-react": "^2.1.10",
    "graphql": "^15.5.1",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "next": "^12.0.7",
    "nookies": "^2.5.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-recaptcha-v3": "^1.9.5",
    "react-intersection-observer": "^8.32.2",
    "react-intl": "^5.24.7",
    "react-paginate": "^7.1.3",
    "react-parallax": "^3.3.0",
    "react-player": "^2.9.0",
    "react-portal": "^4.2.1",
    "react-scroll-parallax": "^2.4.0",
    "react-scrollmagic": "^2.3.0",
    "sharp": "^0.30.6",
    "swiper": "6.7.5",
    "tailwind-hamburgers": "^1.1.1",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.1.21",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/google-map-react": "^2.1.3",
    "@types/gtag.js": "^0.0.8",
    "@types/react-paginate": "^7.1.1",
    "@types/react-portal": "^4.0.4",
    "@types/scrollmagic": "^2.0.2",
    "autoprefixer": "^10.4.7",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "eslint": "7.29.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^4.3.8",
    "jest": "^27.0.6",
    "lint-staged": "^10.5.4",
    "postcss": "^8.4.14",
    "prettier": "^2.2.1",
    "scrollmagic": "^2.0.8",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "tailwindcss": "^3.1.3"
  }
}
